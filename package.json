{
  "name": "openpocket",
  "version": "0.2.0-mvp-ts",
  "private": true,
  "description": "OpenPocket Node.js TypeScript runtime",
  "main": "dist/cli.js",
  "bin": {
    "openpocket": "dist/cli.js"
  },
  "scripts": {
