    "start": "node dist/cli.js"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/node-telegram-bot-api": "^0.64.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
